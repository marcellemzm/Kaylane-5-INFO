    <%Veiculos.forEach(function(veiculo){%>
    <tr>
        <td><%=veiculo.veiCod%></td>
        <td><%=veiculo.veiPlaca%></td>
        <td><%=veiculo.veiCor%></td>
        <td><%=veiculo.veiModel%></td>
        <td><%=veiculo.veiMarca%></td>
        <td><a href="/edtveiculo/<%=veiculo._id%>">Editar</a></td>
        <td><a href="/delveiculo/<%=veiculo._id%>">Delete</a></td>
    </tr>
    <% }) %>
</table>

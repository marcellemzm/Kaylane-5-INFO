<%-include('cabecalho.ejs')%>
<form method="post">
    <label for="id">Id</label>
    <input type="text" name="id" id="id" required readonly value="<%=Empresa._id%>"><br>
    <label for="empCod">Código da Empresa</label>
    <input type="number" name="empCod" id="empCod" required placeholder="Digite o Código da Empresa" 
    value="<%=Empresa.empCod%>"><br>
    <label for="empLocal">Local da Empresa</label>
    <input type="text" name="empLocal" id="empLocal" required placeholder="Informe o Local da Empresa"
    value="<%=Empresa.empLocal%>"><br>
    <label for="empFunc">Numero de Funcionarios da Empresa</label>
    <input type="number" name="empFunc" id="empFunc" required placeholder="Informe o Numero de Funcionarios da Empresa" 
    value="<%=Empresa.empFunc%>"><br>
    <label for="empNome">Nome da Empresa</label>
    <input type="text" name="empNome" id="empNome" required placeholder="Informe o Nome da Empresa" 
    value="<%=Empresa.empNome%>"><br>
    <label for="empTelef">Telefone da Empresa</label>
    <input type="text" name="empTelef" id="empTelef" required placeholder="Informe o Telefone da Empresa" 
    value="<%=Empresa.empTelef%>"><br>
    <input type="submit" value="Adicionar">
</form>
<%-include('rodape.ejs')%>